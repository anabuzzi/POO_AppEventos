<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.senai.sc.eventos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Eventos"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Eventos">
        <activity android:name=".ListarLocaisActivity"></activity>
        <activity android:name=".CadastroLocalActivity" />
        <activity android:name=".CadastroEventoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>